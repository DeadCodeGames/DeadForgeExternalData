{
  "json.schemas": [
    {
      "fileMatch": [
        "/DeadForgeAssets/notes/games/*.jsonc"
      ],
      "url": "./DeadForgeAssets/$schema/note.schema.json"
    },
    {
      "fileMatch": [
        "/DeadForgeAssets/curated/games/*.jsonc",
        "/DeadForgeAssets/official/games/*.jsonc"
      ],
      "url": "./DeadForgeAssets/$schema/games.schema.json"
    }
  ]
}
